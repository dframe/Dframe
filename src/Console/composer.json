    "keywords": [
        "framework",
        "dframe",
        "cli",
        "command",
        "mvc"
    ],
    "homepage": "https://dframeframework.com",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "SÅ‚awomir Kaleta",
            "email": "slaszka@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.3",
        "dframe/dframe": "^4.1"
    },
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "Dframe\\Console\\": "src"
        }
