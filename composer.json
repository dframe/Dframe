{
    "name": "dframe/dframe",
    "description": "PHP Framework - Dframe",
    "keywords": [
        "framework",
        "dframe",
        "mvc"
    ],
    "homepage": "https://dframeframework.com",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "SÅ‚awomir Kaleta",
            "email": "slaszka@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.0",
        "ext-json": "*",
        "psr/simple-cache": "^1.0"
    },
    "suggest": {
        "smarty/smarty": "^3.1",
        "kriswallsmith/assetic": "^1.4",
        "patchwork/jsqueeze": "^2.0",
        "ptachoire/cssembed": "^1.0",
        "natxet/CssMin": "^3.0"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Dframe\\": "src"
        },
        "files": [
            "src/Functions.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Dframe\\Tests\\": "tests"
        }
    },
    "require-dev": {
        "mikey179/vfsStream": "^1.6",
        "phpunit/phpunit": "^6.5",
        "smarty/smarty": "^3.1",
        "kriswallsmith/assetic": "^1.4",
        "patchwork/jsqueeze": "^2.0",
        "ptachoire/cssembed": "^1.0",
        "natxet/CssMin": "^3.0",
        "twig/twig": "^2.5"
    },
    "scripts": {
        "test": [
            "@phpunit"
        ],
        "phpunit": "php vendor/bin/phpunit"
    }
}
